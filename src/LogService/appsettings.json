{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "ServiceBusSink2",
        "pathFormat": "logs/Log-{Date}.log",
        "Filter": {
          "ByExcluding": "FromSource(MyClass)"
        }
      }
    ]
  },
  "ServiceSettings":{
    "ServiceName": "LogService",
    "PostgresConnectionString": "Host=localhost;Port=6000;Database=default_database;Username=username;Password=password"
  },
  "RabbitMQSettings":{
    "Host": "amqp://user:user@rabbitmq-fy:5672"
  }
}
